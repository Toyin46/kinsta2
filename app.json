{
  "expo": {
    "name": "kinsta2",
    "slug": "kinsta2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kinsta2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinsta.kinsta2",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Kinsta to access your camera to take photos and videos.",
        "NSMicrophoneUsageDescription": "Allow Kinsta to record audio for videos.",
        "NSPhotoLibraryUsageDescription": "Allow Kinsta to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Kinsta to save photos to your library.",
        "NSLocationWhenInUseUsageDescription": "Allow Kinsta to access your location.",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "GADApplicationIdentifier": "ca-app-pub-8235065812461074~5241579110"
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      }
    },
    "android": {
      "package": "com.kinsta.kinsta2",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8235065812461074~5241579110"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#00ff88",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new notifications"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "usesCleartextTraffic": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#00ff88",
          "sounds": []
        }
      ],
      "expo-audio",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Kinsta to access your camera to take photos and videos.",
          "microphonePermission": "Allow Kinsta to record audio for videos.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Kinsta to access your photos.",
          "savePhotosPermission": "Allow Kinsta to save photos to your library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Kinsta to access your photos.",
          "cameraPermission": "Allow Kinsta to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Kinsta to use your location."
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.kinsta.kinsta2",
          "enableGooglePay": true
        }
      ],
      "expo-font",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8235065812461074~5241579110",
          "iosAppId": "ca-app-pub-8235065812461074~5241579110",
          "delayAppMeasurementInit": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cc54ae3d-1746-409a-8041-cead41240d56"
      }
    },
    "owner": "toyin",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cc54ae3d-1746-409a-8041-cead41240d56"
    }
  }
}