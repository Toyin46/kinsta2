{
  "expo": {
    "name": "kinsta2",
    "slug": "kinsta2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kinsta2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinsta.kinsta2",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Kinsta to access your camera to take photos and videos.",
        "NSMicrophoneUsageDescription": "Allow Kinsta to record audio for videos.",
        "NSPhotoLibraryUsageDescription": "Allow Kinsta to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Kinsta to save photos to your library.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      }
    },
    "android": {
      "package": "com.kinsta.kinsta2",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "com.google.android.gms.permission.AD_ID",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8235065812461074~5241579110"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#00ff88",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new notifications"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#00ff88",
          "sounds": [],
          "mode": "production"
        }
      ],
      "expo-audio",
      "expo-video",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Kinsta to access your camera to take photos and videos.",
          "microphonePermission": "Allow Kinsta to record audio for videos.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Kinsta to access your photos.",
          "savePhotosPermission": "Allow Kinsta to save photos to your library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Kinsta to access your photos.",
          "cameraPermission": "Allow Kinsta to access your camera."
        }
      ],
      "expo-secure-store",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Allow Kinsta to access your camera",
          "enableCodeScanner": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "react-native-google-mobile-ads"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "https://expo.dev/accounts/toyin/projects/kinsta2/builds/4c1209a3-d231-4166-867c-17ac91953907"
      }
    }
  }
} 
	
